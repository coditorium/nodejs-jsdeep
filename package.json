{
  "name": "jsdeep",
  "version": "1.2.0",
  "description": "Deep operations on json objects",
  "main": "./dist/jsdeep.js",
  "scripts": {
    "lint": "eslint .",
    "build": "npm run build:normal && npm run build:min",
    "build:normal": "webpack",
    "build:min": "webpack --env production",
    "clean": "rimraf ./dist ./coverage",
    "test": "mocha --compilers js:babel-core/register --require './test/setup.js' ./test/**/*.spec.js",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --require './test/setup.js' ./test/**/*.spec.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "ci": "npm run clean && npm run lint && npm run build && npm run coverage",
    "preversion": "npm whoami && npm run ci",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags && npm publish && npm run clean"
  },
  "keywords": [],
  "author": "",
  "license": "WTFPL",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.3.2",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.18.0",
    "eslint-config-coditorium": "^1.3.0",
    "eslint-loader": "^1.7.1",
    "istanbul": "v1.1.0-alpha.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "mocha-metatitles": "^1.0.1",
    "webpack": "^2.3.2",
    "yargs": "^7.0.2"
  }
}
